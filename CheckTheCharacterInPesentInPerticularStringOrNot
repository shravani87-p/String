package string;

public class CheckTheCharacterInPesentInPerticularStringOrNot {
public static void main(String[] args) {
		String str="AbcEe";
		System.out.println(check(str));
}
public static boolean check(String str)
{
	char ch[]=str.toCharArray();
	for(char a:ch)
	{
		if(a=='e')
		{
			return true;
		}
	}
	return false;
}

}
