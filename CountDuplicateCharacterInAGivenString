package string;

public class CountDuplicateCharacterInAGivenString {
public static void main(String[] args) {
	String str="Java is simple and Easy Programming Language";
	System.out.println(countChar(str));
}
public static int countChar(String str)
{
	String res="";
	int count=0;
	for(int i=0;i<str.length();i++)
	{
		char ch=str.charAt(i);
		boolean flag=false;
		for(int j=i+1 ;j<str.length();j++)
		{
			//If you donâ€™t want to count spaces
			if (ch == ' ') 
				continue;

			if(ch==str.charAt(j))
			{
				flag=true;
				break;
			}
		}
		if(flag && res.indexOf(ch)==-1)
		{
			count++;
			res=res+ch;
			
		}
			
	}
	return count;
}
}
