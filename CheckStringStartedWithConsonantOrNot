package string;

public class CheckStringStartedWithConsonantOrNot {
	public static void main(String[] args) {
		String str="I like Java";
		System.out.println(checkString(str));	}

public static boolean checkString(String str)
{
	if(!(str.charAt(0)=='a'|| str.charAt(0)=='e'|| str.charAt(0)=='i'
			|| str.charAt(0)=='o'|| str.charAt(0)=='u'||str.charAt(0)=='A'|| 
			str.charAt(0)=='E'|| str.charAt(0)=='I'
			|| str.charAt(0)=='O'|| str.charAt(0)=='U'))
	{
		return true;
	}
	return false;
}
}
