package string;

public class CountAllCharacterExceptUppercase {
public static void main(String[] args) {
	String str="Java Is Easy";
	countCharEUppercase(str);
}
public static void countCharEUppercase(String str)
{
	int count=0;
	for(char ch: str.toCharArray())
	{
		if(!(ch>='A' && ch<='Z'))
		{
			count++;
		}
	}
	System.out.println("Count is: "+count);
}
}
