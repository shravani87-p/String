package string;

public class PrintFirstTwoDuplicateCharacterInString {
public static void main(String[] args) {
	String str="Java is simple and easy";
	System.out.println(print(str));
}
public static String print(String str)
{
	String res="";
	int count=0;
	for(int i=0;i<str.length();i++)
	{
		char ch=str.charAt(i);
		boolean flag=false;
		for(int j=i+1;j<str.length();j++)
		{
			if(ch==' ')
				continue;  
			if(ch==str.charAt(j))
			{
				flag=true;
				break;
			}
		}
		if(flag && res.indexOf(ch)==-1)
		{
			res=res+ch;
			count++;
		}
		if(count==2)
		{
			break;
		}
	}
	return res;
}
}
