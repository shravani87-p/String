package string;

public class ToStoredOnlyUniqueCharacterOrRemoveDuplicate {
public static void main(String[] args) {
	String str="java";
	System.out.println(store(str));
}
public static String store(String str)
{
	String res="";
	for(int i=0;i<str.length();i++)
	{
		char ch=str.charAt(i);
		if(res.indexOf(ch)==-1)
		{
			res=res+ch;
		}
	}
	return res;
}
}
