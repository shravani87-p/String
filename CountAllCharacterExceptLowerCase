package string;

public class CountAllCharacterExceptLowerCase {
	public static void main(String[] args) {
		String str="Java Is Easy";
		countCharELowercase(str);
	}
	public static void countCharELowercase(String str)
	{
		int count=0;
		for(char ch: str.toCharArray())
		{
			if(!(ch>='a' && ch<='z'))
			{
				count++;
			}
		}
		System.out.println("Count is: "+count);
	}
}
