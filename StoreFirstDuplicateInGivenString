package string;

public class StoreFirstDuplicateInGivenString {
public static void main(String[] args) {
	String str="Java support 100% oop Language";
	System.out.println(store(str));
}
public static String store(String str)
{
	String res="";
	for(int i=0; i<str.length();i++)
	{
		boolean flag=false;
		char ch=str.charAt(i);
		for(int j=i+1;j<str.length();j++)
		{
			if(ch==str.charAt(j))
			{
				flag=true;
				break;
			}
		}
		if(flag && res.indexOf(ch)==-1)
		{
			res=res+ch;
			break;
		}
	}
	return res;
}
}
