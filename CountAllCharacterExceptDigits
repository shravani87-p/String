package string;

public class CountAllCharacterExceptDigits {
public static void main(String[] args) {
	String str="65Java123Is09Easy00";
	countIs(str);
}
public static void countIs(String str)
{
	int count=0;
	for(char ch: str.toCharArray())
	{
		if(!(ch>='0' && ch<='9'))
		{
			count++;
		}
	}
	System.out.println("Count is: "+count);
}
}
