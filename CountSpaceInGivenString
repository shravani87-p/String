package string;

public class CountSpaceInGivenString {
public static void main(String[] args) {
	String str=" Java is easy ";
	countSpace(str);
}
public static void countSpace(String str)
{
	int count=0;
	for(char ch: str.toCharArray())
	{
		if(ch==' ')
			count++;
	}
	System.out.println("count space: "+count);
}
}
